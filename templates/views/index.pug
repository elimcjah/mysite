extends ../layouts/default

block content
	#header
		style(type='text/css', media='screen').
			#hi {
				text-align: center;
				margin-top: 10%;
			}
			
			#javascript {
				margin-top: 5%;
			}

			.card-grid {
				width: 140px;
				height: 140px;
				margin: 20px;
				display: inline-block;
			}

			.card-grid .front, .card-grid .back {
				padding: 10px;
				text-align: center;
				border: 1px transparent;
				border-radius: 25px;
			}
			
			.card-grid .back {
				background-color: #333;
				color: white;
				border: 1px #333 solid;
				border-radius: 25px;
			}

			.front-bithound {
				background-color: #c41230;
			}

			.front-chrome {
				background-color: #FFF;
			}

			.front-docker {
				background-color: #FFF;
			}

			.front-es6 {
				background-color: #F26522;
			}

			.front-eslint {
				background-color: #FFF;
			}

			.front-express {
				background-color: rgba(238, 238, 238, 1.0);
			}

			.front-jasmine {
				background-color: #FFF;
			}
			
			.front-javascript {
				background-color: #F7DF1E;
			}
			
			.front-jest {  
				background-color: #C63D14;
			}

			.front-jquery {
				background-color: #FFF;
			}
			
			.front-keystone {
				background-color: #FFF;
				overflow: hidden;
			}

			.front-mongo {
				background-color: rgba(255, 255, 255, 1.0);
			}

			.front-node {
				background-color: rgba(51, 51, 51, 1.0);
			}

			.front-nodemon {
				background-color: #4F4D3F;
			}

			.front-npm {
				background-color: #CB3837;
			}

			.front-pug {
				background-color: #F2F2F2;
			}
			
			.front-react {
				background-color: #222;
			}

			.front-reactrouter {
				background-color: #222;
			}

			.front-redux {
				background-color: #FFF;
			}

			.front-travis {
				background-color: #FFF;
			}
			
			.front-webpack {
				background-color: #FFF;
			}

			.front-webstorm {
				background-color: #FFF;
			}

			.front-yarn {
				background-color: #2c8ebb;
			}


			
			
			
			
			
			
			
	#body
		div#hi
			h1 Hi, My name is Elijah.
			h2 I build companies & products.
		.container
			h3#javascript Javascript Frameworks, Libraries, and Tools I Use
