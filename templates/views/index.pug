extends ../layouts/default

block content
	#header
		style(type='text/css', media='screen').
			#hi {
				text-align: center;
				margin-top: 10%;
			}
			
			#javascript {
				margin-top: 20%;
			}
			
			h3 {
				margin-top: 5%;
			}

			.card-grid {
				width: 140px;
				height: 140px;
				margin: 20px;
				display: inline-block;
			}

			.card-grid .front, .card-grid .back {
				padding: 10px;
				text-align: center;
				border: 1px transparent;
				border-radius: 25px;
			}
			
			.card-grid .back {
				background-color: #333;
				color: white;
				border: 1px #333 solid;
				border-radius: 25px;
			}
			
			.front-apple {
				background: #1E64F0; /* For browsers that do not support gradients */
				background: -webkit-linear-gradient(#1AD6FD, #1E64F0); /* For Safari 5.1 to 6.0 */
				background: -o-linear-gradient(#1AD6FD, #1E64F0); /* For Opera 11.1 to 12.0 */
				background: -moz-linear-gradient(#1AD6FD, #1E64F0); /* For Firefox 3.6 to 15 */
				background: linear-gradient(#1AD6FD, #1E64F0); /* Standard syntax */
			}

			.front-bithound {
				background-color: #c41230;
			}

			.front-chrome {
				background-color: #FFF;
			}

			.front-docker {
				background-color: #FFF;
			}
			
			.front-envato {
				background-color: #FFF;
			}

			.front-es6 {
				background-color: #F26522;
			}

			.front-eslint {
				background-color: #FFF;
			}

			.front-express {
				background-color: rgba(238, 238, 238, 1.0);
			}
			
			.front-flurry {
				background-color: #FFF;
			}
			
			.front-itunesconnect {
				background: #1E64F0; /* For browsers that do not support gradients */
				background: -webkit-linear-gradient(#1AD6FD, #1E64F0); /* For Safari 5.1 to 6.0 */
				background: -o-linear-gradient(#1AD6FD, #1E64F0); /* For Opera 11.1 to 12.0 */
				background: -moz-linear-gradient(#1AD6FD, #1E64F0); /* For Firefox 3.6 to 15 */
				background: linear-gradient(#1AD6FD, #1E64F0); /* Standard syntax */
			}

			.front-jasmine {
				background-color: #FFF;
			}
			
			.front-javascript {
				background-color: #F7DF1E;
			}
			
			.front-jest {  
				background-color: #C63D14;
			}

			.front-jquery {
				background-color: #FFF;
			}
			
			.front-jsdoc {
				background-color: #F1F1F1;
				font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
				font-size: 37px;
				font-weight: bold;
				color: #006FBB;
				line-height: 90px;
			}
			
			.front-keystone {
				background-color: #FFF;
				overflow: hidden;
			}

			.front-mongo {
				background-color: rgba(255, 255, 255, 1.0);
			}

			.front-node {
				background-color: rgba(51, 51, 51, 1.0);
			}

			.front-nodemon {
				background-color: #4F4D3F;
			}

			.front-npm {
				background-color: #CB3837;
			}
			
			.front-objectivec {
				font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
				font-size: 37px;
				color: #ffffff;
				line-height: 90px;
				background: #1E64F0; /* For browsers that do not support gradients */
				background: -webkit-linear-gradient(#1AD6FD, #1E64F0); /* For Safari 5.1 to 6.0 */
				background: -o-linear-gradient(#1AD6FD, #1E64F0); /* For Opera 11.1 to 12.0 */
				background: -moz-linear-gradient(#1AD6FD, #1E64F0); /* For Firefox 3.6 to 15 */
				background: linear-gradient(#1AD6FD, #1E64F0); /* Standard syntax */
			}
			
			.front-php {
				background-color: #FFF;
			}
			
			.front-phpstorm {
				background-color: #FFF;
			}

			.front-pug {
				background-color: #F2F2F2;
			}
			
			.front-rails {
				background-color: #FFF;
			}
			
			.front-react {
				background-color: #222;
			}

			.front-reactrouter {
				background-color: #FFF;
			}

			.front-redux {
				background-color: #FFF;
			}
			
			.front-ruby {
				background-color: #FFF;
			}
			
			.front-rubygems {
				background-color: #FFF;
			}
			
			.front-rubymine {
				background-color: #FFF;
			}
			
			.front-splunk {
				background-color: #66A43F;
				font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
				font-weight: 500;
				font-size: 120px;
				color: #ffffff;
				line-height: 90px;
			}
			
			.front-testflight {
				background-color: #1676BE;
			}
			
			.front-threejs {
				background-color: #F1F1F1;
			}

			.front-travis {
				background-color: #FFF;
			}
			
			.front-webgl {
				background-color: #FFF;
			}
			
			.front-webpack {
				background-color: #FFF;
			}

			.front-webstorm {
				background-color: #FFF;
			}
			
			.front-wordpress {
				background-color: #FFF;
			}
			
			.front-xcode {
				background: #1E64F0; /* For browsers that do not support gradients */
				background: -webkit-linear-gradient(#1AD6FD, #1E64F0); /* For Safari 5.1 to 6.0 */
				background: -o-linear-gradient(#1AD6FD, #1E64F0); /* For Opera 11.1 to 12.0 */
				background: -moz-linear-gradient(#1AD6FD, #1E64F0); /* For Firefox 3.6 to 15 */
				background: linear-gradient(#1AD6FD, #1E64F0); /* Standard syntax */
			}

			.front-yarn {
				background-color: #2c8ebb;
			}
			
			.front-yii {
				background-color: #FFF;
			}
			
			
			
			
			
